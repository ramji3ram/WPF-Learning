
select distinct ClaimSummaryUID,
cs.ClaimNumber,cs.memberuid,cs.alternatecustomerid_cs,
cs.ClientNumber_cs,
cs.PlatformCode,
cs.BillTypecode,
TotalPaidAmount,
cs.ProcessDate,
cs.ClaimReceiptDate,
cs.FirstDateOfService,
cs.LastDateOfService,
cs.LengthOfStay,
cs.CheckDate,
cs.alternatecustomerid_cs,
cs.LineofBusinessCode,
cs.PlaceOfService,
cs.GROUPEDDRGCode_CS,
cs.BilledDRGCode,
cs.controlid_cs,
cs.NetworkID,
cs.ParCode,
cs.sourcecausecode_cs,cs.DischargeStatusCode,
cs.ClaimSubmissionMethod_cs,
cs.HospitalPhysicianType,cs.LoadMonth,
cs.ProviderTaxID,
cs.ProviderAssignedID,
cs.AdmissionDate
,cs.DischargeDate,
cs.RenderingProviderNPI
,cs.ASOIndicatorCode,
cs.ProviderMedicareNumber,
cs.AdmissionHour,
cs.DischargeHour,
cs.AdmissionSourceCode
,cs.AdmissionType,
cs.ProviderType,
cs.ciscontractid_cs
from humana_prod_wh.medclaim cs
join humana_prod_wh.medclaimsummary mcs1 on 
mcs1.claimnumber = cs.claimnumber
where mcs1.TotalPaidAmount > 100000  and PayeeCode not in ('i','e','r') 
and cs.LoadMonth >= 201801
